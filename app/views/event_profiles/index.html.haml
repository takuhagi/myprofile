= render 'layouts/header'
.list-page
  .jumbotron.jumbotron-fluid.p-4
    .container
      %h1.display-4.mb-4
        イベント一覧
      %p
        This is an introduction of recommended events.
      

      .search-list-page
        %hr.my-4
          = search_form_for @q do |f|
            .sorts
              = f.select( :sorts, { '名前順': 'name asc', '名前逆順': 'name desc', '更新の古い順': 'updated_at asc', '更新の新しい順': 'updated_at desc' }, { selected: params[:q][:sorts] }, { onchange: 'this.form.submit()', class: 'form-control sort'} ) 
              .form-group.keyword
                -# = f.label :name_or_explanation, "キーワード検索", class: "mt-2"
                = f.text_field :title_or_explanation_cont, class: "form-control ", placeholder: "キーワード検索"
              .form-group
                = f.submit "検索", class: "btn btn-primary"
  .container
    .row
      -# ここから佐久間さん指定の6枚
      - if @priority_event_profile1 != nil
        .col-lg-3.col-md-4.col-sm-6.col-xs-2.mb-4
          = link_to event_profile_path(@priority_event_profile1.id) do
            .card.commonButton
              .card-image
                -# - if event_profile.image.present?
                -#   = image_tag(event_profile.image.url, alt: "メイン写真", class: "card-img-top no_image")
                -# - else
                - if @priority_event_profile1.image.present?
                  = image_tag(@priority_event_profile1.image.url, class: "card-img-top")
                - else
                  %img.card-img-top{alt: "...", src: "/assets/no_image.png"}/
              .card-body{style: "position: relative; height: 240px;"}
                %h5.card-title
                  -# = event_profile.title
                  -# カードタイトル１
                  - if @priority_event_profile1 != nil
                    = @priority_event_profile1.title
                  - else
                    イベント名未定
                - if @priority_event_profile1 != nil
                  .small.text-muted
                    = "投稿者：#{@priority_event_profile1.user.nickname}"
                    -# 投稿者の名前
                .small.text-muted{style: "margin-bottom: 12px"}
                  -# = "最終更新：#{time_ago_in_words(event_profile.updated_at)}前"
                  -# 最終更新日時
                  - if @priority_event_profile1 != nil
                    = "最終更新：#{time_ago_in_words(@priority_event_profile1.updated_at)}前"
                    -# = @priority_event_profile1.updated_at
                %p.card-text{style: "margin-bottom: 0.75rem;"}
                  -# = event_profile.explanation.truncate(45)
                  -# カードの説明文です
                  - if @priority_event_profile1 != nil
                    = @priority_event_profile1.explanation.truncate(30)
                    -# - if event_profile.start.present?
                    - if @priority_event_profile1.start.present?
                      %p.card-text{style: "margin-bottom: 0.75rem;"}
                        %small.text-muted
                          - if @priority_event_profile1 != nil
                            開始予定 :
                            = @priority_event_profile1.start.strftime("%Y-%m-%d %H:%M")
                          -# - else
                          -#   開始予定 :◯◯:◯◯
                          -# %small.text-muted
                          -#   = event_profile.start.strftime("%Y-%m-%d %H:%M")
                %div{style: "position: absolute; right: 16px; bottom: 12px"}
